@using EjercicioMandaloriano_Unidad8_SGEMP.Models.entidades
@model EjercicioMandaloriano_Unidad8_SGEMP.ViewModels.VMMisiones
@{
    ViewData["Title"] = "Index";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>

</head>
<body>
    <form method="post">
        <select name="titulo">
            @foreach (clsMision mision in Model.getListadoSinDescripcion())
            {
                <option value="@mision.Titulo">@mision.Titulo</option>
            }
        </select>
        <input type="submit" value="Ver Detalles">
    </form>
    <p>@Html.ViewData["titulo"]</p>
    <p>@Html.ViewData["descripcion"]</p>

</body>
</html>